<h1>Celestial Entities</h1>

<ul>
<% @celestial_entities.each do |celestial_entity| %>
  <li onclick='window.open("<%= url_for(celestial_entity) %>");' >
    <%= image_tag 'celestial_entities/' + celestial_entity.name + '.jpg' %>
    <div><%= celestial_entity.name %></div>
  </li>
<% end %>
</ul>

